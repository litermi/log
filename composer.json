{
    "name": "litermi/logs",
    "description": "package to log routines and sql query",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "log",
        "database"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/database": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/http": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^7.20|^8.19|^9.0|^10.0|^11.0",
        "illuminate/config": "^7.20|^8.19|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Litermi\\Logs\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Litermi\\Logs\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "LogConsole": "Litermi\\Logs\\Facades\\LogConsoleFacade"
            }
        }
    },
    "authors": [
        {
            "name": "Cirel Ramos",
            "email": "cirelramos@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
